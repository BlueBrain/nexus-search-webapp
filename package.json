    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-image-lightbox": "^5.1.0",
    "react-infinite-scroll-component": "^4.5.3",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
