{
  "name": "nexus-search-webapp",
  "description": "Service which allows you to search Nexus platform",
  "version": "0.1.8",
  "author": "Blue Brain Nexus",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": "github:bluebrain/nexus-explorer",
  "homepage": "https://github.com/BlueBrain/nexus-search-webapp#readme",
  "scripts": {
    "build": "export NODE_ENV=production && parcel build server/index.js --target node --out-dir dist/server && parcel build public/index.pug --public-url ./ --out-dir dist/public && envsub --env npm_package_version dist/public/index.html",
    "prepare:pipeline": "npm run prepare:client && npm run prepare:server",
    "prepare:client": "mkdir -p dist-client/dist && cp dist/public/* dist-client/dist && cp -r docker/client dist-client/docker",
    "prepare:server": "mkdir -p dist-server/dist && cp dist/server/* dist-server/dist && cp -r docker/server dist-server/docker && cp package.json dist-server/dist && cp package-lock.json dist-server/dist",
    "build-docker:client": "npm run build && npm run prepare:client && docker build -f dist-client/docker/client/Dockerfile -t ${npm_package_name}-client .",
    "build-docker:server": "npm run build && npm run prepare:server && docker build -f dist-server/docker/server/Dockerfile -t ${npm_package_name}-server .",
    "babel-preset-env": "^1.7.0",
    "dev": "npm run start:client & npm run start:server",
    "start:client": "export NODE_ENV=development && $(babel-node getenv.js) && parcel public/index.pug -p 8000 --no-source-maps",
    "start:server": "export NODE_ENV=development && nodemon --exec babel-node server/index.js",
    "start": "npm run dev",
    "start-docker": "docker run --rm -p 8000:8000 ${npm_package_name}",
    "dotenv": "export NODE_ENV=development && node_modules/.bin/babel-node getenv.js",
    "mapping": "export NODE_ENV=development && node_modules/.bin/babel-node --presets=env scripts/generateMapping",
    "neuron-pipe": "NODE_ENV=development node_modules/.bin/babel-node --presets=env scripts/neuron-pipe/index.js",
    "neuron-create-project": "NODE_ENV=development node_modules/.bin/babel-node --presets=env scripts/neuron-pipe/create-project/index.js",
    "serve": "node dist/index.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "echo \"Not implemented yet\" && exit 0"
  },
  "dependencies": {
    "@bbp/nexus-js-helpers": "^0.4.3",
    "@bbp/nexus-react": "0.0.17",
    "@bbp/nexus-search-extensions": "^1.0.2",
    "@tweenjs/tween.js": "^17.2.0",
    "antd": "^3.9.2",
    "batch-promises": "0.0.3",
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "chroma-js": "^1.3.7",
    "components": "^0.1.0",
    "connected-react-router": "^4.3.0",
    "cors": "^2.8.4",
    "distinct-colors": "^1.0.4",
    "dotenv": "^6.1.0",
    "dygraphs": "^2.1.0",
    "elasticsearch": "^15.1.1",
    "event-emitter": "^0.3.5",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "express-ws": "^4.0.0",
    "fast-deep-equal": "^2.0.1",
    "gsap": "^2.0.1",
    "history": "^4.7.2",
    "jszip": "^3.1.5",
    "kafka-node": "^3.0.1",
    "kafka-pipeline": "^1.0.0",
    "kafkajs": "^1.4.3",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "morphoviewer": "^0.1.9",
    "node-fetch": "^2.3.0",
    "pretty-bytes": "^5.1.0",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "randomcolor": "^0.5.3",
    "react": "^16.4.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-cursor-position": "^2.5.0",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-json-view": "^1.19.0",
    "react-motion": "^0.5.2",
    "react-paginate": "^5.2.3",
    "react-redux": "^5.0.7",
    "react-responsive-modal": "^3.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-spring": "^5.4.0",
    "react-svg": "^4.1.8",
    "react-tabs": "^2.2.2",
    "react-tooltip": "^3.6.1",
    "react-websocket": "^2.0.0",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12",
    "styled-jss": "^2.2.3",
    "swcmorphologyparser": "^0.1.1",
    "three": "^0.96.0",
