{
  "name": "nexus-search-webapp",
  "description": "Service which allows you to search Nexus platform",
  "version": "0.0.3",
  "author": "Blue Brain Nexus",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": "github:bluebrain/nexus-explorer",
  "homepage": "https://github.com/BlueBrain/nexus-search-webapp#readme",
  "scripts": {
    "build": "export NODE_ENV=production && parcel build server/index.js --target node --out-dir dist/server && parcel build public/index.pug --public-url ./ --out-dir dist/public && envsub --env npm_package_version dist/public/index.html",
    "build-docker:client": "npm run build && docker build -f docker/client/Dockerfile -t ${npm_package_name}-client .",
    "build-docker:server": "npm run build && docker build -f docker/server/Dockerfile -t ${npm_package_name}-server .",
    "dev": "export NODE_ENV=development && ( babel-watch server/index.js --presets env --watch server/index.js & $(babel-node getenv.js) && parcel public/index.pug -p 8000 --no-cache )",
    "start": "babel-node server/index.js --presets env & parcel public/index.pug -p 8000",
    "start-docker": "docker run --rm -p 8000:8000 ${npm_package_name}",
    "dotenv": "export NODE_ENV=development && node_modules/.bin/babel-node getenv.js",
    "mapping": "export NODE_ENV=development && node_modules/.bin/babel-node --presets=env scripts/generateMapping",
    "neuron-pipe": "NODE_ENV=development node_modules/.bin/babel-node --presets=env scripts/neuron-pipe/index.js",
    "neuron-create-project": "NODE_ENV=development node_modules/.bin/babel-node --presets=env scripts/neuron-pipe/create-project/index.js",
    "serve": "node dist/index.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "@bbp/nexus-js-helpers": "^0.4.3",
    "@bbp/nexus-react": "0.0.17",
    "@bbp/nexus-search-extensions": "^1.0.2",
    "@tweenjs/tween.js": "^17.2.0",
    "antd": "^3.9.2",
    "batch-promises": "0.0.3",
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "chroma-js": "^1.3.7",
    "components": "^0.1.0",
    "connected-react-router": "^4.3.0",
    "cors": "^2.8.4",
    "distinct-colors": "^1.0.4",
    "dygraphs": "^2.1.0",
    "elasticsearch": "^15.1.1",
    "event-emitter": "^0.3.5",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "gsap": "^2.0.1",
    "history": "^4.7.2",
    "jszip": "^3.1.5",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "morphoviewer": "^0.1.5",
    "pretty-bytes": "^5.1.0",
    "querystring": "^0.2.0",
    "react": "^16.4.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-cursor-position": "^2.5.0",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-json-view": "^1.19.0",
    "react-motion": "^0.5.2",
    "react-paginate": "^5.2.3",
    "react-redux": "^5.0.7",
    "react-responsive-modal": "^3.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-spring": "^5.4.0",
    "react-svg": "^4.1.8",
    "react-tabs": "^2.2.2",
    "react-tooltip": "^3.6.1",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12",
    "styled-jss": "^2.2.3",
    "three": "^0.96.0",
    "underscore": "^1.9.1",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-watch": "^2.0.7",
    "dns-cache": "^2.0.0",
    "dotenv": "^6.0.0",
    "envsub": "^3.1.0",
    "eslint": "^5.1.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.10.0",
    "inquirer": "^6.2.0",
    "jest": "^23.4.1",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "ts-jest": "^23.0.0",
    "typescript-babel-jest": "^1.0.5"
  },
  "jest": {
    "collectCoverage": false
  }
}
