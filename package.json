{
  "name": "nexus-web",
  "description": "The interface of Blue Brain Nexus, the open-source knowledge graph for data-driven science.",
  "version": "1.3.5",
  "author": "Blue Brain project",
  "license": "Apache-2.0",
  "main": "dist/server.js",
  "repository": "github:bluebrain/nexus-explorer",
  "url": "https://github.com/BlueBrain/nexus/issues/new?labels=frontend,nexus-web",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=* webpack --mode development --config-name server && node dist/server.js",
    "build": "NODE_ENV=production webpack --mode production",
    "test": "jest",
    "cy:run": "cypress run",
    "test-ui": "API_ENDPOINT=http://test start-server-and-test start http://localhost:8000 cy:run",
    "codecov": "codecov",
    "lint": "tslint --project tsconfig.json",
    "style": "prettier --check \"./src/**/*.{ts,tsx,js,jsx,less}\"",
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
