{
  "name": "nexus-search",
  "description": "Service which allows you to search Nexus platform",
  "version": "0.0.1",
  "author": "Bluebrain project",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": "github:bluebrain/nexus-explorer",
  "scripts": {
    "build": "parcel build public/index.html --public-url ./ && envsub --env npm_package_version dist/index.html",
    "build-docker": "npm run build && docker build -f docker/Dockerfile -t ${npm_package_name} .",
    "start": "parcel public/index.html -p 8000",
    "start-docker": "docker run --rm -p 8000:8000 ${npm_package_name}",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint ."
  },
  "dependencies": {
    "@bbp/nexus-js-helpers": "^0.4.2",
    "@bbp/nexus-react": "^0.0.6",
    "history": "^4.7.2",
    "moment": "^2.22.1",
    "pretty-bytes": "^4.0.2",
    "querystring": "^0.2.0",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-json-view": "^1.16.1",
    "react-motion": "^0.5.2",
    "react-paginate": "^5.2.2",
    "react-redux": "^5.0.7",
    "react-responsive-modal": "^2.0.1",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-tabs": "^2.2.2",
    "react-tooltip": "^3.5.0",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "2.2.0",
    "store": "^2.0.12",
    "styled-jss": "^2.2.2"
  },
  "devDependencies": {
    "babel-preset-react-app": "^3.1.1",
    "envsub": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "parcel-bundler": "^1.7.1"
  }
}
